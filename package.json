{
  "name": "assemble-fs",
  "description": "Assemble plugin to add methods to assemble for working with the file system, like src, dest, copy and symlink.",
  "version": "0.4.7",
  "homepage": "https://github.com/assemble/assemble-fs",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "assemble/assemble-fs",
  "bugs": {
    "url": "https://github.com/assemble/assemble-fs/issues"
  },
  "license": "MIT",
  "files": [
    "index.js",
    "utils.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "assemble-handle": "^0.1.2",
    "extend-shallow": "^2.0.1",
    "is-registered": "^0.1.4",
    "is-valid-instance": "^0.1.0",
    "lazy-cache": "^2.0.1",
    "stream-combiner": "^0.2.2",
    "through2": "^2.0.1",
    "vinyl-fs": "^2.4.3"
  },
  "devDependencies": {
    "buffer-equal": "^1.0.0",
    "expect": "^1.20.1",
    "graceful-fs": "^4.1.4",
    "gulp-format-md": "^0.1.9",
    "mocha": "^2.5.2",
    "rimraf": "^2.5.2",
    "should": "^8.4.0",
    "sinon": "^1.17.4",
    "templates": "^0.18.3",
    "vinyl": "^1.1.1"
  },
  "keywords": [
    "assemble",
    "assembleplugin",
    "file",
    "fs",
    "system",
    "vinyl"
  ],
  "verb": {
    "run": true,
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": [
        "verb",
        "generate",
        "assemble",
        "assemble-loader",
        "assemble-render-file",
        "assemble-streams"
      ]
    },
    "reflinks": [
      "verb"
    ],
    "lint": {
      "reflinks": true
    }
  }
}
