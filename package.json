{
  "name": "assemble-fs",
  "description": "Assemble plugin to add methods to assemble for working with the file system, like src, dest, copy and symlink.",
  "version": "0.3.7",
  "homepage": "https://github.com/assemble/assemble-fs",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "assemble/assemble-fs",
  "bugs": {
    "url": "https://github.com/assemble/assemble-fs/issues"
  },
  "license": "MIT",
  "files": [
    "index.js",
    "utils.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "assemble-handle": "^0.1.0",
    "extend-shallow": "^2.0.1",
    "lazy-cache": "^1.0.3",
    "stream-combiner": "^0.2.2",
    "through2": "^2.0.0",
    "vinyl-fs": "^2.3.1"
  },
  "devDependencies": {
    "buffer-equal": "^1.0.0",
    "graceful-fs": "^4.1.2",
    "gulp-format-md": "^0.1.5",
    "mocha": "*",
    "rimraf": "^2.5.0",
    "should": "*",
    "sinon": "^1.17.2",
    "templates": "^0.10.7",
    "vinyl": "^1.1.1"
  },
  "keywords": [
    "assemble",
    "assembleplugin",
    "file",
    "fs",
    "system",
    "vinyl"
  ],
  "verb": {
    "related": {
      "list": [
        "assemble",
        "assemble-loader",
        "assemble-render-file",
        "assemble-streams"
      ]
